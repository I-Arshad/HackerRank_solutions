 #!/bin/python3

import math
import os
import random
import re
import sys

#
# Complete the 'miniMaxSum' function below.
#
# The function accepts INTEGER_ARRAY arr as parameter.


def miniMaxSum(arr):
    t=[]
    for i in arr:
        x=-i
        for j in arr:
            x+=j
        t.append(x)
    print(min(t),max(t)) 

if __name__ == '__main__':

    arr = list(map(int, input().rstrip().split()))

    miniMaxSum(arr)
